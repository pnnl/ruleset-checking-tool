{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Enumerations from Title 24",
    "description": "Schema addition for ASHRAE 229 related to enumerations related to California Title 24 publications.",
    "definitions": {
        "RulesetModelOptions2019T24Com": {
            "type": "string",
            "enum": [
                "PROPOSED_SIZING",
                "BASELINE_SIZING",
                "BASELINE"
            ],
            "descriptions": [
                "The proposed building model used for sizing",
                "The baseline building model used for sizing",
                "The baseline building model"
            ]
        },
        "RulesetModelOptions2019T24Res": {
            "type": "string",
            "enum": [
                "STANDARD",
                "BASELINE"
            ],
            "descriptions": [
                "The standard building model",
                "The baseline building model"
            ]
        },
        "ConstructionClassificationOptions2019T24": {
            "type": "string",
            "enum": [
                "MASS_LIGHT",
                "MASS_HEAVY",
                "RAISED_MASS"
            ],
            "descriptions": [
                "Mass, light",
                "Mass, heavy",
                "Raised mass"
            ],
            "notes": [
                "Light mass walls are walls with a heat capacity of at least 7.0 Btu/ft^2^-^o^F and less than 15.0 Btu/ft^2^-^o^F",
                "Heavy mass walls are walls with a heat capacity of at least 15.0 Btu/ft^2^-^o^F",
